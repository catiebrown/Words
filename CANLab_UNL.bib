  year = {2016},
  keywords = {Emotions,Female,Humans,Male,Young Adult,Adult,Reaction Time,Adolescent,Affect,Age Factors,Aged,Aging,Facial Expression,Facial Recognition,Middle Aged},
  pages = {730-739},
