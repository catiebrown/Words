  author = {Mattek, Alison M. and Whalen, Paul J. and Berkowitz, Julia L. and Freeman, Jonathan B.},
  year = {2016},
  keywords = {Emotions,Affective Valence,Facial Expressions,Human Channel Capacity,Perceptual Motor Processes,Stimulus Ambiguity},
  pages = {929-936},
  file = {/Users/cbrown/Zotero/storage/ETKI4RWN/Mattek et al. - 2016 - Differential effects of cognitive load on subjecti.pdf;/Users/cbrown/Zotero/storage/EGBLQIJY/2016-22471-001.html}
}

@article{neta_neural_2013,
